/* index.wxss */
@import "../../wxParse/wxParse.wxss";
page{
  background: gainsboro;
}
.nav{
  background: white;
}
.nav>view{
  display: inline-block;
  margin-right: 25px;
  width: 30%;
  height: 40px;
  line-height: 40px;
  text-align: center;
  font-size: 17px;
  font-weight: bold;
}
.banner image{
  height: 200rpx;
}
#mapbox{
  padding: 0 40rpx;
}
#mapbox{
  height: 400rpx;  
}
#myMap{
  width:100%;
  height: 400rpx;
}
.case{
  padding: 30rpx 30rpx;
}
.case .title{
  height: 70rpx;
  line-height: 70rpx;
  font-size: 35rpx;
  color: #0767C8;
}
.bechosen{
 border-bottom: 4rpx solid rgb(7,103,200);
}
.intro{
  padding: 0 30rpx 50rpx;
  background: white;
  margin-bottom: 20rpx;
  box-shadow:0 2rpx 5rpx #888888;
}
.intro>view:first-of-type{
  text-align: center;
  padding: 20rpx 0;
  font-size: 30rpx;
  color: black;
  vertical-align: top;
}


.content view,.content{
  color: black;
  font-size: 25rpx;
  line-height: 45rpx;
}
